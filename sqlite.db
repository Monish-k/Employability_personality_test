CREATE TABLE "Entry" ( "ID" INTEGER NOT NULL DEFAULT 1 UNIQUE, "Timestamp" TEXT NOT NULL, "Job" TEXT NOT NULL, "FullName" TEXT NOT NULL, "Email" TEXT NOT NULL, "PhoneNo" INTEGER NOT NULL, "Type" TEXT NOT NULL, "Interaction" TEXT NOT NULL, "Interaction_likelyhood" INTEGER NOT NULL, "Information" TEXT NOT NULL, "Information_likelyhood" INTEGER NOT NULL, "Decision" TEXT NOT NULL, "Decision_likelyhood" INTEGER NOT NULL, "Structure" TEXT NOT NULL, "Structure_likelyhood" INTEGER NOT NULL, PRIMARY KEY("ID" AUTOINCREMENT) )
CREATE TABLE "JobsQuestions" ( "ID" INTEGER NOT NULL UNIQUE, "Job" TEXT NOT NULL UNIQUE, "Questions" TEXT NOT NULL UNIQUE, PRIMARY KEY("ID" AUTOINCREMENT) )
CREATE TABLE "User" ( "ID" INTEGER NOT NULL DEFAULT 1 UNIQUE, "FullName" TEXT NOT NULL UNIQUE, "UserName" TEXT NOT NULL UNIQUE, "Password" TEXT NOT NULL UNIQUE, "Email" TEXT NOT NULL UNIQUE, "PhoneNo" INTEGER NOT NULL UNIQUE, PRIMARY KEY("ID" AUTOINCREMENT) )
CREATE TABLE sqlite_sequence(name,seq)
